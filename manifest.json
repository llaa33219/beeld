{
    "manifest_version": 3,
    "name": "엔트리-beeld",
    "version": "1.0",
    "description": "엔트리에서 이미지를 간편하게 업로드할 수 있게 해줍니다.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://playentry.org/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }